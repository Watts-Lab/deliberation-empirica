{
  "headers": [
    {
      "source": "**/*.@(jpg|jpeg|gif|png|txt|md|yaml|mp3)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Origin, X-Requested-With, Content-Type, Accept, Range"
        }
      ]
    }
  ]
}
