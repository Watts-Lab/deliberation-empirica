{
  "dependencies": {
    "@empirica/admin": "^1.0.0-beta.2",
    "axios": "^0.27.2",
    "marked": "^4.0.17",
    "minimist": "1.2.5",
    "rand-seed": "1.0.1"
  },
  "devDependencies": {
    "esbuild": "0.14.25"
  },
  "name": "deliberation-empirica-server",
  "private": true,
  "scripts": {
    "dev": "node --trace-warnings --enable-source-maps src/index.mjs",
    "build": "rsync -aP --exclude=node_modules --exclude=*.js -m src/* dist/ && esbuild src/index.mjs --platform=node --define:process.env.NODE_ENV='\"production\"' --bundle --minify --outfile=dist/index.js",
    "serve": "node --trace-warnings --enable-source-maps index.js"
  },
  "volta": {
    "node": "16.4.1",
    "npm": "7.19.1",
    "yarn": "1.22.15"
  }
}
