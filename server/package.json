{
  "dependencies": {
    "@empirica/admin": "^1.0.0-beta.2",
    "minimist": "1.2.5",
    "node-fetch": "^3.2.4",
    "rand-seed": "1.0.1"
  },
  "devDependencies": {
    "esbuild": "0.14.25"
  },
  "name": "deliberation-empirica-server",
  "private": true,
  "scripts": {
    "build": "rsync -aP --exclude=node_modules --exclude=*.js -m src/* dist/ && esbuild src/index.js --platform=node --bundle --minify --outfile=dist/index.js",
    "dev": "node --trace-warnings --enable-source-maps src/index.js",
    "serve": "node --trace-warnings --enable-source-maps index.js"
  },
  "type": "module",
  "volta": {
    "node": "16.4.1",
    "npm": "7.19.1",
    "yarn": "1.22.15"
  }
}
